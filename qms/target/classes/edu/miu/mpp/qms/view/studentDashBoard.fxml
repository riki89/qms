<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.miu.mpp.qms.controller.StudentDashBoard">
   <children>
      <Pane prefHeight="500.0" prefWidth="500.0">
         <children>
            <VBox fillWidth="false" prefHeight="500.0" prefWidth="500.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#logoutBtnAction" text="Logout">
                     <VBox.margin>
                        <Insets left="420.0" top="10.0" />
                     </VBox.margin>
                  </Button>
                  <Label fx:id="studentNameLbl" alignment="CENTER" contentDisplay="CENTER" text="Hello Moneeba," textAlignment="CENTER">
                     <VBox.margin>
                        <Insets left="200.0" top="20.0" />
                     </VBox.margin>
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
                  <Button mnemonicParsing="false" onAction="#doQuizBtnAction" prefHeight="40.0" prefWidth="312.0" text="Do The Quiz">
                     <VBox.margin>
                        <Insets left="100.0" top="100.0" />
                     </VBox.margin>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#myReportBtnAction" prefHeight="40.0" prefWidth="312.0" text="My Report">
                     <VBox.margin>
                        <Insets left="100.0" top="20.0" />
                     </VBox.margin>
                  </Button>
               </children>
            </VBox>
         </children>
      </Pane>
   </children>
</AnchorPane>
