<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.miu.mpp.qms.controller.ProfessorDashBoard">
   <children>
      <Pane layoutX="10.0" layoutY="10.0" prefHeight="500.0" prefWidth="500.0">
         <children>
            <VBox fillWidth="false" prefHeight="500.0" prefWidth="500.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#logoutBtnAction" text="Logout">
                     <VBox.margin>
                        <Insets left="420.0" top="10.0" />
                     </VBox.margin>
                  </Button>
                  <Label fx:id="professorNameLbl" alignment="CENTER" contentDisplay="CENTER" text="Hello Professor," textAlignment="CENTER">
                     <VBox.margin>
                        <Insets left="200.0" top="20.0" />
                     </VBox.margin>
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
                  <Button fx:id="createQuiz" mnemonicParsing="false" onAction="#createQuizBtnAction" prefHeight="40.0" prefWidth="312.0" text="Create Quiz">
                     <VBox.margin>
                        <Insets left="100.0" top="100.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="professorReport" mnemonicParsing="false" onAction="#myReportBtnAction" prefHeight="40.0" prefWidth="312.0" text="My Report">
                     <VBox.margin>
                        <Insets left="100.0" top="20.0" />
                     </VBox.margin>
                  </Button>
               </children>
            </VBox>
         </children>
      </Pane>
   </children>
</AnchorPane>
