<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="600.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.miu.mpp.qms.controller.QuizManagementController">
   <children>
      <Button fx:id="logoutBtn" alignment="CENTER_RIGHT" contentDisplay="RIGHT" mnemonicParsing="false" onAction="#logoutAction" text="Logout">
         <VBox.margin>
            <Insets left="320.0" top="20.0" />
         </VBox.margin>
      </Button>
      <HBox prefHeight="50.0" prefWidth="200.0">
         <children>
            <Label prefHeight="17.0" prefWidth="83.0" text="Start Time">
               <HBox.margin>
                  <Insets left="20.0" top="20.0" />
               </HBox.margin>
            </Label>
            <DatePicker fx:id="startime">
               <HBox.margin>
                  <Insets top="15.0" />
               </HBox.margin>
            </DatePicker>
         </children>
         <VBox.margin>
            <Insets top="60.0" />
         </VBox.margin>
      </HBox>
      <HBox prefHeight="50.0" prefWidth="200.0">
         <children>
            <Label prefHeight="17.0" prefWidth="83.0" text="End Time">
               <HBox.margin>
                  <Insets left="20.0" top="20.0" />
               </HBox.margin>
            </Label>
            <DatePicker fx:id="endTime">
               <HBox.margin>
                  <Insets top="15.0" />
               </HBox.margin>
            </DatePicker>
         </children>
      </HBox>
      <HBox prefHeight="86.0" prefWidth="400.0">
         <children>
            <Label prefHeight="17.0" prefWidth="81.0" text="Duration (min)">
               <HBox.margin>
                  <Insets left="20.0" top="20.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="duration" prefHeight="27.0" prefWidth="171.0">
               <HBox.margin>
                  <Insets left="20.0" top="15.0" />
               </HBox.margin>
            </TextField>
         </children>
      </HBox>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button fx:id="addBtn" alignment="CENTER" mnemonicParsing="false" onAction="#switchToQuestion" prefHeight="32.0" prefWidth="135.0" text="Add Questions">
               <HBox.margin>
                  <Insets bottom="20.0" left="130.0" top="30.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button fx:id="goBackBtn" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#backToProfDashBoard" prefHeight="35.0" prefWidth="73.0" text="Back">
               <HBox.margin>
                  <Insets left="160.0" top="30.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
